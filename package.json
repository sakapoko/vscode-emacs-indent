{
    "name": "vscode-emacs-indent",
    "displayName": "vscode-emacs-indent-v2",
    "description": "emacs like tab indent",
    "version": "0.0.9",
    "license": "MIT",
    "publisher": "kshartman",
    "author": "kshartman",
    "contributors": [
        { "name": "sakapoko" },
        { "name": "kshartman" }
    ],
    "repository": {
	"type": "git",
	"url": "https://github.com/kshartman/vscode-emacs-indent.git"
    },
    "engines": {
	"vscode": "^1.74.0"
    },
    "categories": [
	"Other"
    ],
    "activationEvents": [],
    "main": "./out/extension.js",
    "contributes": {
	"commands": [
	    {
	    "command": "emacs-indent.reindentCurrentLine-v2",
	    "title": "reindent current line"
	}
	],
	"keybindings": [
	    {
	    "key": "tab",
	    "command": "emacs-indent.reindentCurrentLine-v2",
	    "when": "editorTextFocus"
	}
	]
    },
    "scripts": {
	"vscode:prepublish": "npm run compile",
	"compile": "tsc -p ./",
	"lint": "eslint \"src/**/*.ts\"",
	"watch": "tsc -watch -p ./",
	"pretest": "npm run compile",
	"test": "node ./out/test/runTest.js",
        "package": "npm run lint && npm run compile && vsce package"
    },
    "devDependencies": {
	"@types/glob": "^7.1.1",
	"@types/mocha": "^10.0.1",
	"@types/node": "^16.11.7",
	"@types/vscode": "^1.73.0",
	"@typescript-eslint/eslint-plugin": "^5.42.0",
	"@typescript-eslint/parser": "^5.42.0",
	"@vscode/test-electron": "^2.3.0",
	"eslint": "^8.26.0",
	"glob": "^7.1.4",
	"mocha": "^10.2.0",
	"source-map-support": "^0.5.12",
       	"typescript": "^5.0.2",
	"ci": "^2.2.0"
    },
    "dependencies": {
    }
}
